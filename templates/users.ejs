<%- include("partials/header", { path, title: "Users"}) %>
<div class="text-center flex flex-col justify-center items-center px-4 py-16">
    <div>
        <h1 class="text-4xl font-bold">Users</h1>
        <p class="text-gray-500">List of users</p>
        <table class="text-center w-full border border-gray-500 bg-gray-800 shadow-md text-xl">
            <tr>
                <thead class="bg-gray-700">
                <th class="w-1/3 border border-gray-600 font-semibold p-4">User </th>
                <th class="w-2/3 border border-gray-600 font-semibold p-4"> </th>
                </thead>
            </tr>
            <% users.forEach(row => { %>
                <tr>
                    <td class="border border-gray-500 p-4 text-gray-200"><%= row.name.replace('"', '').replace('"', '') %></td>
                </tr>
            <% }) %>

        </table>    </div>
</div>

<%- include("partials/footer") %>